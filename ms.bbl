  \verb 10.1111/j.1461-0248.2012.01772.x
  \endverb
  \field{issn}{1461-0248}
  \field{pages}{584\bibrangedash 593}
  \field{title}{{Accounting for dispersal and biotic interactions to
  disentangle the drivers of species distributions and their abundances.}}
